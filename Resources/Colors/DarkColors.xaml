<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="MainColor">#242B3C</Color>
    <SolidColorBrush x:Key="MainColorBrush" Color="{StaticResource MainColor}"/>

    <Color x:Key="MainTextColor">#fff</Color>
    <SolidColorBrush x:Key="MainTextColorBrush" Color="{StaticResource MainTextColor}"/>
</ResourceDictionary>